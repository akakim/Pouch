<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pouch">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.IntroActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.MainActivity"
            android:label="@string/title_activity_main" />
        <activity
            android:name=".ui.SettingActivity"
            android:label="@string/title_activity_setting" />
        <activity
            android:name=".ui.RegistBrandActivity"
            android:label="@string/title_activity_regist_brand" />
        <activity android:name=".ui.ItemPouchActivity" />
        <activity android:name=".ui.ColorPalatteActivity" />
        <activity android:name=".ui.EventInfoActivity" />
        <activity android:name=".ui.SearchActivity"></activity>
    </application>

</manifest>
